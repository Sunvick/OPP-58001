        "class Student:\n",
        "   def __init__ (self, Name, Student_No, Age, School, Course):\n",
        "    self.Name = Name\n",
        "    self.Student_No = Student_No\n",
        "    self.Age = Age\n",
        "    self.School = School\n",
        "    self.Course = Course\n",
        "   def self (self):\n",
        "    print (\"Name: \", self.Name)\n",
        "    print (\"Student Number: \", self.Student_No)\n",
        "    print (\"Age : \", self.Age)\n",
        "    print (\"School: \", self.School)\n",
        "    print (\"Course: \", self.Course)\n",
